{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "origamiui/custom-toggle",
  "version": "0.1.0",
  "title": "Toggle",
  "category": "origamiui",
  "description": "A custom Toggle Button block.",
  "attributes": {
    "toggleName": {
      "type": "string",
      "default": ""
    },
    "toggleTarget": {
      "type": "string",
      "default": ""
    },
    "styles": {
      "type": "object",
      "default": {
        "base": {
          "display": {
            "overflow": "---",
            "opacity": "---",
            "zIndex": "---",
            "visible": {
              "sm": "---",
              "md": "---",
              "lg": "---"
            }
          }
        }
      }
    }
  },
  "supports": {
    "html": false,
    "layout": false,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": true
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "shadow": true,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "background": {
      "backgroundImage": true,
      "backgroundSize": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true
    }
  },
  "style": "oui-css",
  "editorStyle": "oui-editor",
  "textdomain": "origamiui",
  "editorScript": "file:./build/index.js"
}